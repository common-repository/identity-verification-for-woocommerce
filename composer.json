{
  "name": "ctrl-alt-dylan/real-id-wc",
  "description": "The Real ID WooCommerce plugin",
  "type": "library",
  "require": {
    "php": "7.4.*||8.*",
    "yohang/finite": "^1.2",
    "monolog/monolog": "^2.3",
    "guzzlehttp/guzzle": "^7.5",
    "php-di/php-di": "^6.3",
    "twig/twig": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "RealID\\": "includes"
    },
    "files": [
      "includes/helpers.php",
      "includes/Container.php"
    ]
  },
  "require-dev": {
    "overtrue/phplint": "^2.0"
  },
  "config": {
    "bin-dir": "bin"
  }
}
